{
    "name": "fedejuret/dto-builder",
    "description": "PHP Library to build DTO's using attributes",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Fedejuret\\DtoBuilder\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Federico Juretich",
            "email": "fedejuret@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "pestphp/pest": "^3.7",
        "codelytv/coding-style": "^1.3",
        "projektgopher/whisky": "^0.7.1"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest",
        "ecs:check": "./vendor/bin/ecs check",
        "post-install-cmd": [
            "php post_install.php"
        ]
    }
}
